#include <iostream>

using namespace std;

string SPADE = "\xe2\x99\xa0";
string HEART = "\xe2\x99\xa5";
string CLUBS = "\xe2\x99\xa3";
string DIMON = "\xe2\x99\xa6";

string SUITS [] = {SPADE,HEART,CLUBS,DIMON};


int main(){
	 

	


	for( int i = 0; i <4; i++){
		 
		cout<<i<<'\t'<<SUITS[i]<<endl;
	}


	//string a = "\xe2\x99\xa5";
	//cout<<SPADE<<endl;

	return 0;
}
